    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=6527beb662bc0b0de39c9b41"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <script
      src="/-/media/allianzgi/ap/taiwan/event/retire/js/webflow.js"
      type="text/javascript"
    ></script>
    <script
      src="https://code.jquery.com/jquery-1.12.4.min.js"
      integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ="
      crossorigin="anonymous"
    ></script>
    <script>
      function gopmt() {
        var x_rate = 0; //每期利率
        var x_per = 12; //月12，季4，半年2，一年1
        var x_nper = 0; //期數
        var x_pv = 0; //期初金額
        var x_fv = 0; //期末金額
        if ($("#thinktotal").val() == "") {
          alert("請輸入預期目標");
          return false;
        }
        if ($("#thinkyear2").val() == "" || $("#thinkyear2").val() == " ") {
          alert("請選擇投資的時間");
          return false;
        }
        if ($("#currentmoney").val() == "") {
          alert("請輸入目前存款");
          return false;
        }
        if ($("#hoperate").val() == "") {
          alert("請選擇希望的投資報酬率");
          return false;
        }
        x_rate = Math.round($("#hoperate").val());
        x_per = 12;
        x_nper = Math.round($("#thinkyear2").val()) * 12;
        // x_pv = Math.round(0 - $("#currentmoney").val() * 10000);
        x_pv = Math.round(0 - 0 * 10000);
        x_fv = Math.round(
          $("#thinktotal").val() * 10000 - $("#currentmoney").val() * 10000
        );
        gocalresult(
          Math.round(0 - pmt(x_rate / 100 / 12, x_nper, x_pv, x_fv, 0))
        );
      }
      function gocalresult(a) {
        $("#calresultprice").html(a);
        $("#calresultrate").html($("#hoperate").val());
      }
      /**
       * Copy of Excel's PMT function.
       * Credit: https://stackoverflow.com/questions/2094967/excel-pmt-function-in-js
       *
       * @param rate_per_period       The interest rate for the loan.
       * @param number_of_payments    The total number of payments for the loan in months.
       * @param present_value         The present value, or the total amount that a series of future payments is worth now;
       *                              Also known as the principal.
       * @param future_value          The future value, or a cash balance you want to attain after the last payment is made.
       *                              If fv is omitted, it is assumed to be 0 (zero), that is, the future value of a loan is 0.
       * @param type                  Optional, defaults to 0. The number 0 (zero) or 1 and indicates when payments are due.
       *                              0 = At the end of period
       *                              1 = At the beginning of the period
       * @returns {number}
       */
      function pmt(
        rate_per_period,
        number_of_payments,
        present_value,
        future_value,
        type
      ) {
        future_value = typeof future_value !== "undefined" ? future_value : 0;
        type = typeof type !== "undefined" ? type : 0;
        if (rate_per_period != 0.0) {
          // Interest rate exists
          var q = Math.pow(1 + rate_per_period, number_of_payments);
          return (
            -(rate_per_period * (future_value + q * present_value)) /
            ((-1 + q) * (1 + rate_per_period * type))
          );
        } else if (number_of_payments != 0.0) {
          // No interest rate, but number of payments exists
          return -(future_value + present_value) / number_of_payments;
        }
        return 0;
      }
    </script>
    <script>
      $("#resetBtn").click(function () {
        $("#thinktotal").val("700");
        $("#currentmoney").val("10");
      });
    </script>
  </div>
