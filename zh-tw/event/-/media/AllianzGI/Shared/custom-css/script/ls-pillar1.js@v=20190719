var Ani={tree:function(n){scrollFn.fade(n,["tree-transparency","tree-growth","tree-performance","tree-megatrends"],aniDelay/2)},living:function(n){switch(n){case"city-living-1":setTimeout(function(){aniCounter($("#city-living-1 .ani-counter"))},aniDelay/2+aniDelayBetween);break;case"city-living-2":setTimeout(function(){aniCounter($("#city-living-2 .ani-counter"))},aniDelay/2+1.5*aniDelayBetween);break;case"city-living-3":setTimeout(function(){aniCounter($("#city-living-3 .ani-counter"))},aniDelay/2+2*aniDelayBetween);break;case"city-living-4":setTimeout(function(){aniCounter($("#city-living-4 .ani-counter"))},aniDelay/2+2.5*aniDelayBetween)}},walking:function(n){"block-walk"==n&&setTimeout(function(){aniCounter($("#block-walk .ani-counter"))},1*aniDelay)},map:function(n){"block-map"==n&&(setTimeout(function(){aniCounter($("#block-map .ani-counter-1"))},aniDelay/2),setTimeout(function(){aniCounter($("#block-map .ani-counter-2"))},aniDelay/2*1.5))},energy:function(n){function e(){var i=300;$("#energy-ani .ani-img").each(function(n){var e=n+1;i+=150,setTimeout(function(){$("#energy-ani").attr("data-ani","ani-"+e)},i)})}"energy-run"==n&&(e(),setInterval(function(){e()},1500))},result:function(n){"result-money"==n&&setTimeout(function(){aniCounter($("#result-money .ani-counter"))},aniDelay/2),"result-financial"==n&&setTimeout(function(){aniCounter($("#result-financial .ani-counter"))},aniDelay),"result-gov"==n&&(setTimeout(function(){aniCounter($("#result-gov .ani-counter-1"))},1.2*aniDelay),setTimeout(function(){aniCounter($("#result-gov .ani-counter-2"))},1.2*aniDelay))},lock:function(n){if("mobile"!=browser.layout()){0<=["lock-trigger-1","lock-trigger-2","lock-trigger-3","lock-trigger-4","lock-trigger-5","lock-trigger-6","lock-trigger-7","lock-trigger-8","lock-trigger-9","lock-trigger-10"].indexOf(n)&&$(".lock-item[data-trigger="+n+"]").addClass("lock-active")}},hexagon:function(n){"mobile"!=browser.layout()?"block-hexagon-items"==n&&(setTimeout(function(){aniCounter(