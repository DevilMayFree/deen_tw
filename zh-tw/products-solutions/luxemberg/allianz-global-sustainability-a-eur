<script id='fund-documents-template' type="text/x-handlebars-template">
    {{#unless IsSuppressed}}
    <div class="l-container l-container--no-gutters">
        <div class="l-grid u-margin-top-2m u-margin-bottom-xl c-fund-documents" data-globallockmsg="{{PopupMessage}}">
            <div class="l-grid__row">
                <div class="l-grid__column-12 u-text-align-center u-hidden" tabindex="0">
                    <h2 class="c-heading u-margin-bottom-xl">{{Title}}</h2>
                </div>
                <div class="l-grid__column-12">
                    <div class="l-grid__row">
                        {{#if KeyDocumentsData}}
                        <div class="l-grid__column-medium-fit align-self-start">
                            <div class="c-radio u-margin-bottom-s">
                                <input id="keyDocumentsCheckbox" class="c-radio__input" name="documentsRadio" data-value="m-document-key-view" type="radio" value="keyDocuments" checked="" tabindex="0">
                                <label for="keyDocumentsCheckbox" class="c-radio__label">
                                    <span class="c-radio__label--text">{{KeyDocuments}}</span>
                                </label>
                            </div>
                        </div>

                        <div class="l-grid__column-medium-fit align-self-start">
                            <div class="c-radio u-margin-bottom-s">
                                <input id="allDocumentsCheckbox" class="c-radio__input" name="documentsRadio" data-value="m-document-list-view" type="radio" value="allDocuments" tabindex="0">
                                <label for="allDocumentsCheckbox" class="c-radio__label">
                                    <span class="c-radio__label--text">{{AllDocuments}}</span>
                                </label>
                            </div>
