                                            </div>
                                        </th>
                                        {{/if}}
                                        {{/each}}
                                    </tr>
                                </thead>
                                <tbody class="c-table__content">
                                    {{#each this.SelectedBreakdowns.BreakDownValues}}
                                    <tr class="c-table__row" tabindex="0">
                                        <td class="c-table__cell">
                                            <div class="c-table__cell-wrapper" tabindex="0">
                                                <span class="c-color-label">
