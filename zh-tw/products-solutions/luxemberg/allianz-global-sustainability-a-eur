<script id="fund-allocations-template" type="text/x-handlebars-template">
    {{#unless IsSuppressed}}
    <div class="l-container l-container--no-gutters u-margin-top-2m u-margin-bottom-xl">
        <div class="l-grid">
            <div class="l-grid__row align-items-center">
                <div class="l-grid__column-12 u-text-align-center">
                    <h3 class="c-heading u-margin-bottom-2xs" tabindex="0">
                        {{Title}}
                        {{#if ComponentTitleDisclaimerSubScriptValue}}
                        <sup class="superscript">{{{ComponentTitleDisclaimerSubScriptValue}}}</sup>
                        {{/if}}
                    </h3>
                    {{#if AsOfDateText}}
                    <p class="c-copy u-margin-bottom-3m u-text-weight-light" tabindex="0">
                        {{AsOfDateText}} {{AsOfDateValue}}
                    </p>
                    {{/if}}
                </div>
                {{#ifCond DataList.length '>' 1}}
                <div class="l-grid__column-fit justify-content-between u-margin-top-2m">
                    <div class="c-radio-group holdings-align-content-left">
                        {{#each DataList}}
                        <div class="c-radio u-margin-bottom-s">
                            <input id="DataList{{@index}}" class="c-radio__input" data-radio-id="{{@index}}"
                                   type="radio" value="{{RadioLabel}}" name="datalist">
                            <label for="DataList{{@index}}" class="c-radio__label">
                                <span class="c-radio__label--text">{{RadioLabel}}</span>
                            </label>
                        </div>
                        {{/each}}
                    </div>
                </div>
                {{/ifCond}}
                {{#each DataList}}
                <div class="l-grid__column-12 {{#ifCond @index '!=' 0}}u-hidden{{/ifCond}}" id="DataListCont{{@index}}">
                    <div class="l-grid__column-large-8 offset-large-2 l-grid__column-medium-8 offset-medium-2">
                        <div class="c-select2 js-select2 u-margin-bottom-3m" data-value="{{this.SelectedBreakdowns.BreakdownTitle}}" data-for="{{@index}}">
                            <div class="c-select2__selection" tabindex="0" aria-haspopup="listbox">
                                <label class="c-select2__label">{{this.BreakdownLabel}}</label>
                                <div class="c-select2__selection__rendered"></div>
