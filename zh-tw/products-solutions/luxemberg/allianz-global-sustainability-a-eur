                                        {{/if}} {{/each}}
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                {{/each}}
                <div class="l-grid__column-12" tabindex="0">
                    <div class="c-copy c-copy--small" tabindex="0">
                        {{{FootNotes}}}
                    </div>
                </div>

