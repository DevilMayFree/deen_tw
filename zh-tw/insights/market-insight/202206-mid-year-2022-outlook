    <div class="l-container u-margin-top-xl u-margin-bottom-2m agi-rte">
        <script>
      // tab component
      let tabComponent = document.querySelector(".c-tabs__nav");
      // navigation tabs
      let firstTab = document.querySelector(
        "#a4245b57-38e1-41fa-b488-4fa5c833a9d3_0"
      ).parentElement;
      let secondTab = document.querySelector(
        "#a4245b57-38e1-41fa-b488-4fa5c833a9d3_1"
      ).parentElement;
      let thirdTab = document.querySelector(
        "#a4245b57-38e1-41fa-b488-4fa5c833a9d3_2"
      ).parentElement;
      let fourthTab = document.querySelector(
        "#a4245b57-38e1-41fa-b488-4fa5c833a9d3_3"
      ).parentElement;

      // content tabs
      let firstContentTab = document.querySelector(
        "#tab-content-a4245b57-38e1-41fa-b488-4fa5c833a9d3_0"
      );
      let secondContentTab = document.querySelector(
        "#tab-content-a4245b57-38e1-41fa-b488-4fa5c833a9d3_1"
      );
      let thirdContentTab = document.querySelector(
        "#tab-content-a4245b57-38e1-41fa-b488-4fa5c833a9d3_2"
      );
      let fourthContentTab = document.querySelector(
        "#tab-content-a4245b57-38e1-41fa-b488-4fa5c833a9d3_3"
      );

      //functions

      function goToSecondTab() {
        firstTab.classList.remove("active");
        secondTab.classList.add("active");
        firstContentTab.classList.remove("active");
        secondContentTab.classList.add("active");
        tabComponent.scrollIntoView(true);
      }

      function goToThirdTab() {
        secondTab.classList.remove("active");
        thirdTab.classList.add("active");
        secondContentTab.classList.remove("active");
        thirdContentTab.classList.add("active");
        tabComponent.scrollIntoView(true);
      }

      function goToFourthTab() {
        thirdTab.classList.remove("active");
        fourthTab.classList.add("active");
        thirdContentTab.classList.remove("active");
        fourthContentTab.classList.add("active");
        tabComponent.scrollIntoView(true);
      }

      function goToFirstTab() {
        fourthTab.classList.remove("active");
        firstTab.classList.add("active");
        fourthContentTab.classList.remove("active");
        firstContentTab.classList.add("active");
        tabComponent.scrollIntoView(true);
      }
    </script>
    </div>
</div>


