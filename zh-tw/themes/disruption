                    <div class="c-ncarousel js-ncarousel js-ncarousel-one-column" data-slider-infinite="true" data-slidedelay="3000" data-pauseonhover="true" data-autoplay="true" data-transitionspeed="3000">
                        <div class="c-ncarousel__wrapper">
                            <div class="c-ncarousel__slide">
    
