    jQuery(document).ready(function($){
        $('.btn-wrap a').hover(function () {
            var btn = $(this).data('btn');
            $('.tab-overlay-wrap').slick('slickGoTo', btn-1);
            $('.tab-overlay-container').addClass('active');
        }, function(){
            $('.tab-overlay-container').removeClass('active');
        });

        $('.tab-overlay-wrap').slick({
            fade: true,
            arrows: false,
            draggable: false,
            adaptiveHeight: false,
            dots: false,
            autoplay: false,
            responsive: [
                {
                    breakpoint: 768,
                    settings: {
                        dots: true,
                        autoplay: true,
                        autoplaySpeed: 5000,
                    }
                },
                // You can unslick at a given breakpoint now by adding:
                // settings: "unslick"
                // instead of a settings object
            ]
        });

        $('.tab-overlay-wrap').on('beforeChange', function(event, slick, currentSlide,  nextSlide){
            var next_slide = nextSlide+1;
            $('.tab-btn-container .btn-wrap a').removeClass('active');
            $('.tab-btn-container .btn-' + next_slide).addClass('active');
        });

        // for enlarge section
        $('.section-1-container').closest('.agi-rte').attr("style", "width: 100%; padding: 0px; margin-top: 0px; margin-top: 0!important");
    });
